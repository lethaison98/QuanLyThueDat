@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section>
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Quyết định thuê đất</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Trang chủ</a></li>
                        <li class="breadcrumb-item active">Quyết định thuê đất</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
</section>
<section>
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Tìm kiếm quyết định thuê đất</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <div class="row">
                            <div class="col-9 col-md-9 col-lg-9 col-xl-9">
                                <div class="ttls-cm">
                                    <div class="ttls-label">Từ khóa</div>
                                    <div class="ttls-content">
                                        <input type="text" id="txtKEY_WORD" placeholder="Nhập từ khóa tìm kiếm..." class="form-control" data-name="Keyword" />
                                    </div>
                                </div>
                            </div>

                            <div class="col-3 col-md-3 col-lg-3 col-xl-3">
                                <div class="ttls-cm">
                                    <div class="ttls-label">&nbsp;</div>
                                    <div class="ttls-content">
                                        <a class="btn btn-sm btn-primary waves-effect waves-light" id="btnSearchQuyetDinhThueDat">
                                            <span>Tra cứu</span>
                                            <i class="fas fa-search"></i>
                                        </a>
@*                                        <a class="btn btn-sm btn-success waves-effect waves-light" id="btnCreateQuyetDinhThueDat">
                                            <span>Tạo mới</span>
                                            <i class="fas fa-plus"></i>
                                        </a>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Danh sách quyết định thuê đất</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="tblQuyetDinhThueDat" class="table table-bordered table-hover table-striped" style="width: 100%">
                            <thead>
                                <tr>
                                    <th style="width: 5%">STT</th>
                                    <th>Tên doanh nghiệp</th>
                                    <th style="width: 10%">Số quyết định giao đất</th>
                                    <th style="width: 10%">Số quyết định thuê đất</th>
                                    <th style="width: 30%">Nội dung</th>
                                    <th style="width: 10%">Ngày ký</th>
                                    <th style="width: 10%">Thao tác</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
</section>
<div id="modalDetailQuyetDinhThueDat"></div>
<div id="modalViewQuyetDinhThueDat"></div>
@section Scripts{
<script type="text/javascript" src="//cdn.jsdelivr.net/momentjs/latest/moment-with-locales.min.js"></script>
<script src="~/js/controller/QuyetDinhThueDatControl.js"></script>
}