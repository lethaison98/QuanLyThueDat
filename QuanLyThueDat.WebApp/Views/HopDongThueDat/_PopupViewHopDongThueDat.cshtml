@{
    Layout = null;
}
<div class="modal fade" id="popupViewHopDongThueDat" data-backdrop="static" role="dialog">
    <div class="modal-dialog modal-xl">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Chi tiết hợp đồng thuê đất</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body" style="min-height:600px">
                <table id="tblViewHopDongThueDat" class="table table-bordered table-hover table-striped" style="width: 100%">
                    <thead>
                        <tr>
                            <th style="width: 5%">STT</th>
                            <th>Tên doanh nghiệp</th>
                            <th style="width: 10%">Số hợp đồng thuê đất</th>
                            <th style="width: 10%">Ngày ký hợp đồng</th>
                            <th style="width: 10%">Người ký</th>
                            <th style="width: 10%">Cơ quan ký</th>
                            <th style="width: 10%">Thời hạn hợp đồng</th>
                            <th style="width: 10%">Từ ngày</th>
                            <th style="width: 10%">Đến ngày</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Đóng</button>
            </div>
        </div>
    </div>
</div>